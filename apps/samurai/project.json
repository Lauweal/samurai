{
  "root": "apps/samurai",
  "sourceRoot": "apps/samurai/app",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nrwl/expo:start",
      "options": {
        "port": 8081
      }
    },
    "web": {
      "executor": "@nrwl/expo:start",
      "options": {
        "port": 8081,
        "webpack": true
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "nx start samurai"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/samurai/app/environments/environment.ts",
              "with": "apps/samurai/app/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "development": {
          "fileReplacements": [
            {
              "replace": "apps/samurai/app/environments/environment.ts",
              "with": "apps/samurai/app/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "run-ios": {
      "executor": "@nrwl/expo:run",
      "options": {
        "platform": "ios"
      }
    },
    "run-android": {
      "executor": "@nrwl/expo:run",
      "options": {
        "platform": "android"
      }
    },
    "build-ios": {
      "executor": "@nrwl/expo:build-ios",
      "options": {}
    },
    "build-android": {
      "executor": "@nrwl/expo:build-android",
      "options": {}
    },
    "build-web": {
      "executor": "@nrwl/expo:build-web",
      "options": {}
    },
    "build-status": {
      "executor": "@nrwl/expo:build-web",
      "options": {}
    },
    "sync-deps": {
      "executor": "@nrwl/expo:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nrwl/expo:ensure-symlink",
      "options": {}
    },
    "publish": {
      "executor": "@nrwl/expo:publish",
      "options": {}
    },
    "publish-set": {
      "executor": "@nrwl/expo:publish-set",
      "options": {}
    },
    "rollback": {
      "executor": "@nrwl/expo:rollback",
      "options": {}
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/samurai/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/samurai"
      ],
      "options": {
        "jestConfig": "apps/samurai/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
